{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true,
    "node": true
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.common.js"
      }
    }
  },
  "rules": {
    "no-param-reassign": "off",
    "no-alert": "off",
    "comma-dangle": "off"
  }
}